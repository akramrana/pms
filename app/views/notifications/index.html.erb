<p id="notice"><%= notice %></p>

<h1>Notifications</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Projectid</th>
      <th>Description</th>
      <th>Isread</th>
      <th>Createdat</th>
      <th>Updatedat</th>
      <th>Isdeleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notifications.each do |notification| %>
      <tr>
        <td><%= notification.userId %></td>
        <td><%= notification.projectId %></td>
        <td><%= notification.description %></td>
        <td><%= notification.isRead %></td>
        <td><%= notification.createdAt %></td>
        <td><%= notification.updatedAt %></td>
        <td><%= notification.isDeleted %></td>
        <td><%= link_to 'Show', notification %></td>
        <td><%= link_to 'Edit', edit_notification_path(notification) %></td>
        <td><%= link_to 'Destroy', notification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Notification', new_notification_path %>
