<%= form_with(model: project_type, local: true) do |form| %>
<% if project_type.errors.any? %>
<div id="error_explanation">
    <h2><%= pluralize(project_type.errors.count, "error") %>:</h2>

    <ul>
        <% project_type.errors.full_messages.each do |message| %>
        <li><%= message %></li>
        <% end %>
    </ul>
</div>
<% end %>
<div class="row">
    <div class="col-6">
        <%= form.label :typeName, "Project Type" %>
        <%= form.text_field :typeName,class: 'form-control' %>
    </div>
</div>
<div class="clearfix">&nbsp;</div>
<div class="form-group">
    <%= form.submit 'Submit', :class => "btn btn-primary" %>
</div>
<% end %>