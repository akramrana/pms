<p id="notice"><%= notice %></p>

<h1>Issue Images</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issue_images.each do |issue_image| %>
      <tr>
        <td><%= link_to 'Show', issue_image %></td>
        <td><%= link_to 'Edit', edit_issue_image_path(issue_image) %></td>
        <td><%= link_to 'Destroy', issue_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Issue Image', new_issue_image_path %>
