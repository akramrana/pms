<%= form_with(model: @issueImage, data: { type: "json" }, remote: true) do |form| %>
    <div id="error_explanation"></div>

    <div class="row">
        <div class="col-12">
            <%= form.hidden_field :issueId %>
            <%= form.label :image %>
            <%= form.file_field :image %>
        </div>
    </div>
    <div class="clearfix">&nbsp;</div>
    <div class="form-group">
        <%= form.submit 'Submit' , :class=> "btn btn-primary" %>
    </div>
    <% end %>