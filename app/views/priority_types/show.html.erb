<% content_for :title, @priority_type.priorityTypeName %>
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <p id="notice"><%= notice %></p>
                <p class="float-right">
                    <%= link_to 'Update', edit_priority_type_path(@priority_type), class: "btn btn-primary" %>
                    <%= link_to 'Back', priority_types_path, class: "btn btn-info" %>
                </p>
                <table class="table table-striped table-bordered">
                    <tbody>
                        <tr>
                            <th>
                                <strong>Priority Type:</strong>
                            </th>
                            <td>
                                <%= @priority_type.priorityTypeName %>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>